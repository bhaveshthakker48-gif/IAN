<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.BNA" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@android:color/transparent</item>
        <item name="colorPrimaryVariant">@android:color/transparent</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="AlertDialogButtonStyle">
        <item name="android:textColor">@color/white</item>
    </style>
    <!-- styles.xml -->
    <style name="maintaining">
        <item name="android:textSize">15dp</item>
        <item name="android:letterSpacing">0.04</item>
    </style>
    <style name="maintaining_tab">
        <item name="android:textSize">20dp</item>
        <item name="android:letterSpacing">0.04</item>
    </style>
    <!-- styles.xml -->
    <style name="aims_of_bna">
        <item name="android:textSize">17dp</item>
        <item name="android:textColor">#F8F9FC</item>
    </style>
    <style name="aims_of_bna_tab">
        <item name="android:textSize">22dp</item>
        <item name="android:textColor">#F8F9FC</item>
    </style>
    <!-- styles.xml -->
    <style name="view_all">
        <item name="android:textSize">17dp</item>
    </style>
    <style name="view_all_tab">
        <item name="android:textSize">@dimen/_6ssp</item>
    </style>
    <style name="dashboard_button_style">
        <item name="android:layout_width">@dimen/_60sdp</item>
        <item name="android:layout_height">@dimen/_60sdp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textColor">#000000</item>
        <item name="android:background">@null</item>
    </style>
    <style name="dashboard_button_style1">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textColor">#000000</item>
        <item name="android:background">@null</item>
    </style>
    <style name="dashboard_button_style_tab">
        <item name="android:layout_width">@dimen/_50sdp</item>
        <item name="android:layout_height">@dimen/_40sdp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#000000</item>
        <item name="android:background">@null</item>
    </style>

    <dimen name="text_size" />
    <!-- styles.xml -->
